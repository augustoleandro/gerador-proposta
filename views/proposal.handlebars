  {{#with this as |order|}}
    {{#each (chunk items 20)}}
      <div class="page">
        <div class="page-header">
          <img src="https://cnxfzvkctgksepdayrpg.supabase.co/storage/v1/object/sign/files/images/logo_horizontal.png?token=eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJ1cmwiOiJmaWxlcy9pbWFnZXMvbG9nb19ob3Jpem9udGFsLnBuZyIsImlhdCI6MTcyNjY2NTE2MCwiZXhwIjoxODg0MzQ1MTYwfQ.y4eOkMnXn_QnDdCkyE7nB8W0UMJN_P06Z--IHosJAp0&t=2024-09-18T13%3A12%3A40.846Z" alt="Logo Automatize" />
