                  <tr>
                    <th style="text-align: left">Item</th>
                    <th style="text-align: left">Descrição</th>
                    <th style="text-align: right">Quantidade</th>
                    
                    {{!-- Coluna de valor unitário é condicional --}}
                    {{#ifEquals ../../showItemValues "true"}}
                      <th style="text-align: right">Valor/un</th>
                    {{/ifEquals}}
                  </tr>
                </thead>
              {{/if}}
              <tbody>
                {{!-- Iteração pelos itens do chunk atual --}}
                {{#each this}}
                  <tr>
                    {{!-- Numeração sequencial usando helper customizado --}}
                    <td style="font-size: 12px">{{calculateItemNumber @../index @index}}</td>
                    <td style="font-size: 12px">{{name}}</td>
                    <td style="font-size: 12px; text-align: right">{{quantity}}</td>
                    {{!-- Valor unitário exibido condicionalmente --}}
                    {{#ifEquals ../../../showItemValues "true"}}
                      <td style="font-size: 12px; text-align: right">{{formatCurrency value}}</td>
                    {{/ifEquals}}
                  </tr>
                {{/each}}
              </tbody>
            </table>

            {{!-- 
              Lógica corrigida:
              - Valor SEMPRE aparece no final da tabela (qualquer quantidade de itens)
              - Descrição na mesma página apenas para ≤14 itens
              - Para >14 itens, descrição vai para página adicional
            --}}
